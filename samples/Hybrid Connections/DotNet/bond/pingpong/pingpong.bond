namespace Bond.Examples.PingPong;

struct PingRequest
{
    0: string Payload;
    1: uint16 DelayMilliseconds;
}

struct PingResponse
{
    0: string Payload;
}

service PingPong
{
    PingResponse Ping(PingRequest);
}
